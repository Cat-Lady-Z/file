<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lbx.distribution.manageserver.mapper.ParamMapper">

    <!-- 获取paramValue -->
    <select id="queryParamByKey" resultType="java.lang.String" parameterType="java.lang.String">
        SELECT
            param_value
        FROM
            params_info
        WHERE
            param_key = #{paramKey}
        AND switch = 1
    </select>
</mapper>